{
    "name": "tarre/laravel-redis-scout-engine",
    "description": "Redis engine for Laravel Scout",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tariqul Islam",
            "email": "tarre.islam@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tarre\\RedisScoutEngine\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel/scout": "~6.0|~7.0|~8.0|~9.0"
    },
    "suggest": {
        "predis/predis": "PHP library for Redis support (Easy to install)",
        "php-redis": "PHP extension for Redis support (Required php extension, faster than predis)"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tarre\\RedisScoutEngine\\ServiceProvider"
            ]
        }
    }
}
